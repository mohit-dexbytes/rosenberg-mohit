{
  "name": "rosenberg_mongo",
  "version": "1.0.0",
  "description": "New folder",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.5.11",
    "@loopback/context": "^1.23.5",
    "@loopback/core": "^1.10.7",
    "@loopback/openapi-v3": "^1.10.1",
    "@loopback/repository": "^1.15.4",
    "@loopback/rest": "^1.23.0",
    "@loopback/rest-explorer": "^1.4.4",
    "@loopback/service-proxy": "^1.3.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/underscore": "^1.9.4",
    "bcryptjs": "^2.4.3",
    "casbin": "^3.0.7",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "loopback-connector-mongodb": "^4.2.0",
    "mongodb": "^3.3.5",
    "underscore": "^1.9.1",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@loopback/authentication": "^3.2.3",
    "@loopback/authorization": "^0.4.5",
    "@loopback/build": "^2.0.16",
    "@loopback/testlab": "^1.9.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.5",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.2"
  }
}
